{
  "expo": {
    "name": "notes",
    "slug": "notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mynotes.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "myapp://",
          "asyncRoutes": {
            "app": "./app/**/*.tsx"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "myapp://",
        "asyncRoutes": {
          "app": "./app/**/*.tsx"
        }
      },
      "eas": {
        "projectId": "5d2f4723-a88d-4c42-b694-8a76b8744ece"
      }
    }
  }
}
